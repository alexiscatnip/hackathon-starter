extends ../layout

block content
  .page-header
    h3 All foods Available
  table(style='width:100%', border='1')
    tr
      th Id
      th Name
      th time left
      th category
    ul 
      for food in foods
        tr 
          td= food._id
          td= food.name
          td= food.date
          td= food.category
      if !foods
        p DB problem 
      p= foods
